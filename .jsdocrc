{
    "source": {
        "include": [
            "./src"
        ],
        "includePattern": ".+\\.js(doc)?$"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "./README.md",
        "package": "./package.json"
    },
    "plugins": [
        "plugins/markdown"
    ]
}